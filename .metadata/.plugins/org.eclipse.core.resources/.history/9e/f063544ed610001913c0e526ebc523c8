package me.duohui.macaronServer.model;

public class Login {

	String id;
	String password;
	int type;
	
	public Login(String id, String password, int type) {
		super();
		this.id = id;
		this.password = password;
		this.type = type;
	}
	
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getPassword() {
		return password;
	}
	public void setPassword(String password) {
		this.password = password;
	}
	public int getType() {
		return type;
	}
	public void setType(int type) {
		this.type = type;
	}
	
	
}
